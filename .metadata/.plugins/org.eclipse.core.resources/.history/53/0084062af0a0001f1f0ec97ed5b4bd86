package Ejercicio03;

import utilidades.Leer;

public class Principal {

	public static void main(String[] args) {
		int numero;
		System.out.println("Dime un número");
		numero=Leer.datoInt();
        Operaciones o=new Operaciones();
        System.out.println("El número " + numero + " es " + o.posNeg(numero));
        System.out.println("El número " + numero + " es " + o.ParImp(numero));
    }

}
